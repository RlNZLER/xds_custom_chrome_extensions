{
  "name": "pcoc-click-template",
  "version": "1.0.0",
  "manifest_version": 3,
  "action": { "default_popup": "popup.html" },
  "permissions": ["scripting", "activeTab", "downloads", "storage"],
  "web_accessible_resources": [
    {
      "resources": [
        "lib/scoc_template_v2.xlsx",
        "lib/xlsx-populate.min.js"
      ],
      "matches": ["<all_urls>"]
    }
  ]
}
